---
import PageHeader from "@/components/PageHeader.astro";
import Layout from "@/layouts/Layout.astro";

import { StudioCMSRenderer } from 'studiocms:renderer';
import studioCMS_SDK from 'studiocms:sdk';

const page = await studioCMS_SDK.GET.databaseEntry.pages.bySlug('studiocms-for-developers');

if (!page) {
  return Astro.redirect('/404');
}
---
<Layout
	title="StudioCMS for Developers"
	desc="StudioCMS offers a lot of benefits to developers in the Astro ecosystem."
>
	<PageHeader
		title="StudioCMS for Developers"
	/>
	<article>
		<StudioCMSRenderer data={page} />	
	</article>
</Layout>
