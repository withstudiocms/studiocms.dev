---
import PageHeader from '@/components/PageHeader.astro';
import Layout from '@/layouts/Layout.astro';

import { StudioCMSRenderer } from 'studiocms:renderer';
import studioCMS_SDK from 'studiocms:sdk';

const page = await studioCMS_SDK.GET.databaseEntry.pages.bySlug('studiocms-for-enterprises');

if (!page) {
	return Astro.redirect('/404');
}
---
<Layout
	title="StudioCMS for Enterprises"
	desc="StudioCMS offers significant benefits to enterprises looking to streamline their digital content management."
>
	<PageHeader
		title="StudioCMS for Enterprises"
	/>
	<article>
		<StudioCMSRenderer data={page} />	
	</article>
</Layout>
